{
  "expo": {
    "name": "TicketboxExpo",
    "slug": "ticketbox-expo",
    "platforms": ["android", "ios", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ticketbox.expo"
    },
    "android": {
      "package": "com.ticketbox.expo"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
